    "import subprocess\n",
    "import os\n",
    "\n",
    "# 获取所有 Conda 环境的名称\n",
    "result = subprocess.run(['conda', 'env', 'list'], capture_output=True, text=True)\n",
    "lines = result.stdout.split('\\n')[2:]  # 跳过前两行标题\n",
    "envs = [line.split()[0] for line in lines if line.strip()]\n",
    "envs"
   ]
